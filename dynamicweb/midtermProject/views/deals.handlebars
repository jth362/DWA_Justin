
{{#each deals}}


<div class="row" id="deal">
  <div class="col-sm-9">
   <h1><a href="deals/{{slug}}">{{name}}</a></h1>
    <div class="row">
      <div class="col-xs-8 col-sm-6">
        <p>{{address}}</p>
      </div>
        <div class="col-xs-4 col-sm-6">
        <p>${{price}}.00</p>
      </div>
      <div class="col-xs-4 col-sm-6">
        <p>{{category}}</p>
      </div>
      <div class="col-sm-9">
        <p>{{description}}</p>
        </div>
      
    </div>
  </div>
</div>

{{/each}}

